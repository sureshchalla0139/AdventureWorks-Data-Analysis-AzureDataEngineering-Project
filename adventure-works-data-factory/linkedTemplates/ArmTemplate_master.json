{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adventure-works-data-factory"
		},
		"LS_PARQUET_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_PARQUET'"
		},
		"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0213-015359-2u8qlj2j"
		},
		"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://key-valut-adv-works.vault.azure.net/"
		},
		"LS_PARQUET_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adventureworkadlsgen2.dfs.core.windows.net/"
		},
		"LS_SqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "SURESHCHALLA\\SQLEXPRESS"
		},
		"LS_SqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"LS_SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adv_sureshch"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adventure-works-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_PARQUET_accountKey": {
						"value": "[parameters('LS_PARQUET_accountKey')]"
					},
					"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_PARQUET_properties_typeProperties_url": {
						"value": "[parameters('LS_PARQUET_properties_typeProperties_url')]"
					},
					"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"LS_SqlServer_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_server')]"
					},
					"LS_SqlServer_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_database')]"
					},
					"LS_SqlServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}